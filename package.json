{
  "name": "billing-ui",
  "version": "1.58.1",
  "description": "KonturBilling UI components",
  "scripts": {
    "test:ci": "npm run eslint && npm run stylelint && npm test -- --reporter min",
    "test": "mocha \"./test/**/*.test.js\"",
    "test:watch": "mocha watch \"./test/**/*.test.js\"",
    "eslint": "eslint \"./**/**.js\" --cache && eslint \"./**/**.jsx\" --cache",
    "stylelint": "stylelint \"./**/*.scss\" --color --syntax scss",
    "stylefmt": "stylefmt --recursive ./styles & stylefmt --recursive ./components",
    "comb": "gulp sass:comb",
    "prettier": "prettier --print-width 160 --tab-width 4 --write '{components,libs,helpers,test}/**/*.js*'",
    "precommit": "yarn comb",
    "preversion": "yarn eslint && yarn stylelint && yarn test",
    "postversion": "git push && git push --tags",
    "storybook": "cd ./docs && yarn && yarn storybook && cd .."
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:skbkontur/billing-ui.git"
  },
  "authors": "KonturBilling",
  "devDependencies": {
    "add-event-listener": "0.0.1",
    "autoprefixer": "^6.3.3",
    "axios": ">= 0.8 < 1",
    "babel-core": ">= 6.7.4",
    "babel-eslint": ">= 6.0.2",
    "babel-loader": ">= 6.2.4",
    "babel-plugin-add-module-exports": ">= 0.1.2",
    "babel-plugin-transform-runtime": ">= 6.6.0",
    "babel-polyfill": ">= 6.7.4",
    "babel-preset-es2015": ">= 6.6.0",
    "babel-preset-react": ">= 6.5.0",
    "babel-preset-stage-0": ">= 6.5.0",
    "babel-register": "^6.9.0",
    "babel-runtime": ">= 6.6.1",
    "bourbon": ">= 4.2.7",
    "chai": "^3.5.0",
    "classnames": "latest",
    "deep-freeze": "0.0.1",
    "eslint": "3.19.0",
    "eslint-plugin-babel": ">= 3.2.0",
    "eslint-plugin-chai-expect": "^1.1.1",
    "eslint-plugin-jsx-a11y": ">= 0.6.2",
    "eslint-plugin-mocha": "^4.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": ">= 4.3.0",
    "exports-loader": "^0.6.4",
    "flux-standard-action": "^1.2.0",
    "gulp": "^3.9.1",
    "gulp-bom": "^1.0.0",
    "gulp-cached": "^1.1.0",
