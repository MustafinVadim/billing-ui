    "gulp-postcss": "^6.1.0",
    "husky": "^0.11.4",
    "jsdom": "latest",
    "jsdom-global": "latest",
    "mocha": "^2.5.3",
    "moment": "^2.12.0",
    "perfectionist": ">= 2.1.3 < 3",
    "postcss-scss": "^0.1.6",
    "postcss-sorting": "^1.2.1",
    "prettier": "^1.3.1",
    "react": "latest",
    "react-custom-scrollbars": "^4.1.2",
    "react-dom": "latest",
    "react-highlight-words": "^0.8.1",
    "react-onclickoutside": "^5.7.1",
    "reduce-reducers": "^0.1.2",
    "redux-saga": ">= 0.10.4 < 1",
    "stylefmt": "^4.3.1",
    "stylelint": "^7.5.0",
